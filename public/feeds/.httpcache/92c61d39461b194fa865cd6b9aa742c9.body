<script>
  function showDias(daysleft) {
    if (daysleft == 1)
      document.getElementById("dias_text").innerHTML = "dia";
    if (daysleft < 10) daysleft = "0" + daysleft;
    document.getElementById("dias_number").innerHTML = daysleft;
  }

  function showHoras(hoursleft) {
    if (hoursleft == 1)
      document.getElementById("horas_text").innerHTML = "hora";
    else {
      document.getElementById("horas_text").innerHTML = "horas";
    }
    if (hoursleft < 10) hoursleft = "0" + hoursleft;
    document.getElementById("horas").innerHTML = hoursleft;
  }

  function showMinutos(minutesleft) {
    if (minutesleft < 10) minutesleft = "0" + minutesleft;
    document.getElementById("min").innerHTML = minutesleft;
  }

  function showSegundos(secondsleft) {
    if (secondsleft < 10) secondsleft = "0" + secondsleft;
    document.getElementById("seg").innerHTML = secondsleft;
  }

  function hide_by_id(id) {
    document.getElementById(id).style.display = "none";
  }

  function startCountdown(finish_date) {
    setInterval(function () {
      //calculate
      var now = new Date();
      var timeleft = finish_date - now;
      var daysleft = Math.floor(timeleft / (1000 * 60 * 60 * 24));
      //var daysleft = 0;
      var hoursleft = Math.floor((timeleft / (1000 * 60 * 60)) % 24);
      var minutesleft =
          Math.floor((timeleft % (1000 * 60 * 60)) / (1000 * 60));
      var secondsleft =
          Math.floor((timeleft % (1000 * 60)) / 1000);

      if (daysleft <= 0) hide_by_id("dias");
      if (hoursleft <= 0) hide_by_id("horas_block");
      if (minutesleft <= 0 && hoursleft <= 0) hide_by_id("minutos_block");
      showDias(daysleft);
      showHoras(hoursleft);
      showMinutos(minutesleft);
      showSegundos(secondsleft);
      if (now > finish_date) {
        document.getElementById("ribbon").style.display = "none";
      }
    }, 1000);
  }
  var parse = Date.parse("Sep 16, 2025 20:0:0 -0300");
  var finishTime = new Date(parse);
  if (new Date() > finishTime) {
    document.getElementById("ribbon").style.display = "none";
  }
  startCountdown(finishTime);
  document
    .getElementById("close-ribbonBF")
    .addEventListener("click", () => {
      document.getElementById("ribbon").style.display = "none";
      if (typeof set_tabela_anchor === 'function') set_tabela_anchor();
