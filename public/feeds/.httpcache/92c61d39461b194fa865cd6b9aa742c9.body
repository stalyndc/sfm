    }
  }
  function closeIcons(icons, closes) {
    for(let j = 0; j < closes.length; j++) {
      closes[j].classList.add("animate_opacity1");
      icons[j].classList.add("animate_opacity0");
    }
  }
  function showDropdown(element, icons, closes) {
    element.classList.add("animate_heigth");
