	padding-bottom: 56.25%; /* 16:9 */
	padding-top: 25px;
	height: 0;
}
.videoWrapper iframe {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}

.col-lg-3-v2{
	position: relative;
    width: 100%;
	min-height: 1px;
	padding-left: 15px;
    padding-right: 15px
}



@media (min-width:1000px) {
	.col-lg-3-v2{
		flex: 0 0 25%;
		max-width: 25%;
		float: left;
	}
}

.overlay-text-v2
{
	font-size: 34px;
    text-shadow: 0 0 10px rgba(0,0,0,.4);
    font-weight: 500;
}

.disable{
	display:none;

}
button.cta>div {
    border-radius: 25px;
    padding: 10px 30px;
    position: relative;
}
button.cta>div:before {
    content: "";
    position: absolute;
    border: 2px solid #fff;
    width: 100%;
    height: 100%;
    transition: all .15s ease-out;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    border-radius: 25px;
}

button.cta>div span.link:after {
    background: url(/images/arrow-link.svg) center;
    background-repeat: no-repeat;
    right: -10px;
    top: 2px;
}

button.cta>div span {
    font-family: "Noto Sans TC","lucida grande",tahoma,verdana,arial,sans-serif;
    color: #fff;
    font-size: 18px;
    font-weight: 300;
    letter-spacing: 0;
    line-height: 25px;
    position: relative;
}

button.cta>div span.ctaIcon:after {
    content: "";
    width: 20px;
    height: 20px;
    display: inline-block;
    position: relative;
}

button.cta {
    min-height: 50px;
    min-width: 245px;
    border-radius: 30px;
    background: linear-gradient(90deg,#f54c53 0,#f77f84 100%);
    box-shadow: 0 2px 5px 0 rgb(199 56 62 / 30%);
    padding: 5px;
    border: 0;
    outline: 0;
    cursor: pointer;
}
</style>


