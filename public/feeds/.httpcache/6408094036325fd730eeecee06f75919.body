					}	
    				str+="<button type='button' class='list-group-item' onclick='location.href=\"/article/"+autoList.items[i].standardDocumentId+"/"+encodeURI(getUrlHeadline(getStandardDocumentHeadline(autoList.items[i].headlines.items)))+"\";'>";
