							var autoListImageSplit = autoListImageName.split('.');
							if(autoListImageSplit!=null && autoListImageSplit.length>0){
								var finalAutoListImageName = autoListImageSplit[0] + "_135." + autoListImageSplit[1];
								var documentId= autoList.items[i].standardDocumentId;
