								//var imageUrl = "https://resource01-proxy.ulifestyle.com.hk/res/v3/image/manual-list/highlight/"+imageId*5000+"/"+documentId+"/"+finalManualListImageName+"";
							//}
							
					}else{
						var imageUrl="";
					}
					if(type=="video"){
						str+="<button type='button' class='list-group-item' onclick='location.href=\"https://www.ulifestyle.com.hk/video/"+manualList.manualItems.items[i].id+"\";'>";	
    				}else{
    					str+="<button type='button' class='list-group-item' onclick='location.href=\"/article/"+manualList.manualItems.items[i].id+"/"+encodeURI(getUrlHeadline(manualList.manualItems.items[i].headline))+"\";'>";
    			
    				}
					str+="<img src='/images/loader.gif' data-src='"+imageUrl+"' class='list-thumb'>"+manualList.manualItems.items[i].headline+"</button>";
 				}			
    		}
    	}
    	return str;
	}
	
	function getStandardDocumentHeadline(headlinesItem){
		for(j=0;j<headlinesItem.length;j++){
			if(headlinesItem[j].type=="main"){
				return headlinesItem[j].name;
			}
		}
	}
