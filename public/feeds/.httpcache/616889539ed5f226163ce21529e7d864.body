          try {
            const experiment = Didomi.getExperiment();
            return experiment?.group === 'test';
          } catch (_) {
            return false;
          }
        };

        const hasValidConsent = () => {
          try {
            const status = Didomi.getUserStatus();
            const purposes = status?.purposes?.consent?.enabled || [];
            const vendors = status?.vendors?.consent?.enabled || [];
            return purposes.length > 0 && vendors.length > 0;
          } catch (_) {
            return false;
          }
        };

        // Flags evaluados
        const isLoggedIn = getIsUserLoggedIn();
        const isExperiment = getIsExperiment();
        const hasConsent = hasValidConsent();

        // Event listeners
        window.didomiEventListeners = window.didomiEventListeners || [];
        window.didomiEventListeners.push({
          event: 'preferences.clickdisagreetoall',
          listener: () => {
            console.log('[Didomi] Usuario hizo click en "Rechazar todo"');

            if (isExperiment) {
              return;
            }

            if (!isLoggedIn) {
              localStorage.setItem('consentRejectedWhileNotLoggedIn', 'true');
              window.location.href = '/registro';
            } else {
              localStorage.setItem('consentRejectedWhileLoggedIn', 'true');
            }
          },
        });

        window.didomiEventListeners.push({
          event: 'notice.clickdisagree',
          listener: () => {
            if (isExperiment) {
              return;
            }

            if (!isLoggedIn) {
              localStorage.setItem('consentRejectedWhileNotLoggedIn', 'true');
              window.location.href = '/registro';
            } else {
              console.log('[Didomi] Usuario estaba logueado');
              localStorage.setItem('consentRejectedWhileLoggedIn', 'true');
            }
          },
        });

        // Verificación de región (GDPR o no)
        const status = Didomi.getCurrentUserStatus();
        const regulation = status.regulation || 'none';
        const isGDPR = regulation === 'gdpr';

        if (!isGDPR) {
          console.log('[Didomi] Non-GDPR region detected. Auto opt-in.');
          if (window.permutive) {
            window.permutive.consent({ opt_in: true, token: 'infobaeA!' });
          }
          return;
        }

        console.log('[Didomi] GDPR region detected.');

        // Escuchar cambios de consentimiento
        Didomi.on('consent.changed', () => {
          const updatedStatus = Didomi.getCurrentUserStatus();
          const permutiveConsent = updatedStatus.vendors['permutive-9wAsbgwh']?.enabled === true;

          console.log('[Permutive] Consent changed. Vendor enabled:', permutiveConsent);

          if (window.permutive) {
            if (permutiveConsent) {
              window.permutive.consent({ opt_in: true, token: 'infobaeA!' });
            } else {
              window.permutive.consent({ opt_in: false });
            }
          }
        });

