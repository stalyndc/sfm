  if (typeof window !== 'undefined') {
    const getIsUserLoggedIn = () => {
      try {
        const userInfo = JSON.parse(localStorage.getItem('ArcId.USER_INFO') || sessionStorage.getItem('ArcId.USER_INFO') || '{}');
        return Boolean(userInfo.accessToken);
      } catch (_) {
        return false;
      }
    };

    const isInRegistro = window.location.pathname === '/registro' || window.location.pathname === '/registro/';

    window.didomiEventListeners = window.didomiEventListeners || [];
    window.didomiEventListeners.push({
      event: 'notice.shown',
      listener: () => {
        document.body.classList.add('didomi-blocked');
        const wall = document.getElementById('didomi-wall');
        if (wall) wall.style.display = 'block';
      },
    });

    window.didomiEventListeners.push({
      event: 'notice.hidden',
      listener: () => {
        document.body.classList.remove('didomi-blocked');
        const wall = document.getElementById('didomi-wall');
        if (wall) wall.style.display = 'none';
      },
    });

    window.didomiOnReady = window.didomiOnReady || [];
    window.didomiOnReady.push((Didomi) => {
      try {
        // Helpers con Didomi
