<script>
$(document).ready(function() {
    var table = $('#transmittalsTable').DataTable({
        "pageLength": 50,
        "order": [[1, "desc"]],
        "searching": true, // Enable searching but hide the default box
        "paging": true,
        "columnDefs": [
            { "type": "date", "targets": 1 }
        ],
        "dom": '<"top"l>rt<"bottom"ip><"clear">' // Hide default search box
    });

    var currentSearchTerm = '';
    var currentYearFilter = '';

    // Search functionality
    var searchEl = document.getElementById('customSearch');
    if (searchEl) {
        searchEl.addEventListener('input', function() {
            currentSearchTerm = this.value;
            applyFilters();
        });
    }

    // Populate year filter
    var yearEl = document.getElementById('yearFilter');
    if (yearEl) {
        var yearCol = table.column(5);
        var years = [];
        yearCol.data().unique().sort().each(function (d) {
            if (d && d.trim() !== "") {
                years.push(d);
            }
        });
        
        years.forEach(function(year) {
            var option = document.createElement('option');
            option.value = year;
            option.textContent = year;
            yearEl.appendChild(option);
        });

        yearEl.addEventListener('change', function() {
            currentYearFilter = this.value;
            applyFilters();
        });
    }

    function applyFilters() {
        // Clear previous searches
        table.search('').columns().search('');
        
        // Apply global search if there's a search term
        if (currentSearchTerm) {
            table.search(currentSearchTerm);
        }
        
        // Apply year filter if selected
        if (currentYearFilter) {
            table.column(5).search('^' + currentYearFilter + '$', true, false);
        }
        
        // Redraw
        table.draw();
    }
});
</script>		
    
	




