    <!-- MBP SCRIPT -->
    <script type="text/javascript">
        google.charts.load("current", { packages: ["corechart"] });
        google.charts.setOnLoadCallback(drawMBPChart);

        async function drawMBPChart() {
            try {
            // Pobieramy dane JSON przez fetch
            const response = await fetch("/vox.php");
            const voxData = await response.json();

            // Zamieniamy obiekt na tablicę [klucz, wartość]
            const voxDataArray = Object.entries(voxData.dane);

            // Tworzymy DataTable
            const data = new google.visualization.DataTable();
            data.addColumn("string", "Data");
            data.addColumn("number", "Kurs");

            // Dodajemy wiersze
            voxDataArray.forEach(([key, value]) => {
                if (value > 0) data.addRow([key, parseFloat(value)]);
            });

            // Opcje wykresu
            const options = {
                chartArea: { left: 30, right: 0, top: 10, bottom: 10 },
                title: "none",
                curveType: "function",
                legend: "none",
            };

            // Rysujemy wykres
            const chart = new google.visualization.LineChart(
                document.getElementById("mbp")
            );
            chart.draw(data, options);
            } catch (error) {
            console.error("Błąd przy pobieraniu danych:", error);
            }
        }

        // Rysowanie wykresu przy zmianie rozmiaru okna
        window.addEventListener("resize", drawMBPChart);
    </script>


    <!-- WEATHER SCRIPT -->
    <script type="text/javascript">
        var temp = document.getElementById('temp');

        //Week days from start date
        function dayOfTheWeek(Month, day, Year) {
            const weekday = [
                "Mondag",
                "Diensdag",
                "Mittdag",
                "Donnersdag",
                "Freedag",
                "Samsdag",
                "Sonnsdag"
            ];
            return weekday[new Date('${Month}/${day}/${Year}').getDay()];
        };

        //Function for fetching data from API
        function fetchWeatherData() {
            fetch(`https://api.weatherapi.com/v1/current.json?key=3e4f8dc7e4ca422e9f0191740241903&q=Ottawa`)
            .then(response => response.json())
            .then(data => {
                console.log(data);

                //Temperature and weather condition
                temp.innerHTML = data.current.temp_c + "&#176;"
                conditionOutput.innerHTML = data.current.condition.text;

                //Date with extracted M,d,Y into individual variables
                const date = data.location.localtime;
                const Y = parseInt(date.substr(0, 4));
                const M = parseInt(date.substr(5, 2));
                const d = parseInt(date.substr(8, 2));

                //Reformating date from original format
                dateOutput.innerHTML = `${dayOfTheWeek(M, d, Y)} ${M} ${d}. ${Y}`

                //Getting corresponding icon url for the weather and extracting it
                const iconId = data.current.condition.icon.substr("//cdn.weatherapi.com/weather/64x64/".length);

                //Weather details
                cloudOutput.innerHTML = data.current.cloud + "%";
                humidityOutput.innerHTML = data.current.humidity + "%";
                windOutput.innerHTML = data.current.wind_kph + "km/h";

                //Default time of day
                let timeOfDay = "day";

                //Unique id for each weather condition
                const code = data.current.condition.code;

                //Change to night if its night time in the city
                if(!data.current.is_day) {
                    timeOfDay = "night";
                }
                app.style.opacity = "1";
                    })
                }
    </script>

