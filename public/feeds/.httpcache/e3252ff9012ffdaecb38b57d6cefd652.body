    window.addEventListener("userInteraction", function (event) {
        if (event.detail.action === "loginSuccess") {
            window.digitalData.user.hhid = event.detail.hhid;
            window.digitalData.user.mvpd = event.detail.mvpd;
            window.digitalData.user.isAuthenticated = event.detail.isAuthenticated;
            window.analyticsDataLayer.hhid = event.detail.hhid;
            let autoAuth = localStorage.getItem("autoAuth");
            if (autoAuth && autoAuth === 'true') {
                News.App.QuantumAnalytics.autoLoginSuccess(event.detail.hhid, event.detail.mvpd);
            } else {
                News.App.QuantumAnalytics.firesQuantumLoginStopSuccess(event.detail.hhid, event.detail.mvpd);
            }
        }
    });

    function updateDigitalData(newCampaignElements){
        window.digitalData.page.mktgCampaignShown = 'yes';
        newCampaignElements.forEach(newElement => {
            let newCampaignName;
            let newCampaignContent = newElement.querySelector('.campaign__content');
            if (newCampaignContent) newCampaignName = newCampaignContent.dataset.campaignName; 
            if (newCampaignName && window.digitalData.page.mktgCampaignName) {
                window.digitalData.page.mktgCampaignName = window.digitalData.page.mktgCampaignName + "," + newCampaignName;
            } else if (newCampaignName) {
                window.digitalData.page.mktgCampaignName = newCampaignName;
            }
        })
    }

    function findNewMatchingElements(nodes) {
        const matches = [];
        nodes.forEach(node => {
            if (!(node instanceof Element)) return;
            if (node.classList.contains('campaign--container')) {
                matches.push(node);
            }
            // check descendants
            const descendant = node.querySelector('.campaign--container');
            if (descendant && descendant.length) matches.push(descendant);
        })
        return matches;
    }

    const campaignObserver = new MutationObserver((mutationsList) => {
        for (const mutation of mutationsList) {
            if (mutation.type === 'childList' && mutation.addedNodes.length) {
                const newCampaignElements = findNewMatchingElements(Array.from(mutation.addedNodes));
                if (newCampaignElements.length) updateDigitalData(newCampaignElements);
            }
        }
    })

    campaignObserver.observe(document.documentElement || document.body, {
        childList: true,
        subtree: true,
    });

    // Initial handler in case some campaign elements already exist in DOM
    findNewMatchingElements(Array.from(document.querySelectorAll('.campaign--container')));
    
    document.addEventListener("DOMContentLoaded", () => {
        campaignObserver.disconnect();
    });

    /**
     * workaround to get the hhid that is coming in late
     * NEWS-1055
     *
     **/
    var _interval = setInterval(fetchLateHHID, 3000);

    function fetchLateHHID() {
        try {
            var auth = Cookies.get('authenticated')
            if (typeof auth !== 'undefined' && auth === 'true') {
                var retHhid = Cookies.get('player-hhid');
                if (typeof retHhid === "undefined") {
                    retHhid = "";
                } else if (window.digitalData.user.hhid !== "" && window.analyticsDataLayer.hhid !== "") {
                    clearInterval(_interval);
                } else {
                    $('#analyticsDataLayer').attr('data-hhid', retHhid);
                    window.analyticsDataLayer.hhid = retHhid;
                    window.analyticsDataLayer.mvpd = Cookies.get('player-mvpd');
                    window.digitalData.user.hhid = retHhid;
                    window.digitalData.user.isAuthenticated = 'Yes';
                    window.digitalData.user.mvpd = Cookies.get('player-mvpd');
                }
            } else {
                clearInterval(_interval);
            }
        } catch (e) {
        }

    }

</script>
