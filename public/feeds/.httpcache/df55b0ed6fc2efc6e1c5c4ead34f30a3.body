<style>

/**
 * Unopened chat
*/

.embeddedServiceHelpButton {
    z-index: 5;
}

.embeddedServiceHelpButton .helpButton {
    height: 2.50rem !important;
    border-radius: 0 !important;
    bottom: 0 !important;
}

.embeddedServiceHelpButton .helpButton .uiButton {
   border-radius: 0;
}

.embeddedServiceHelpButton .helpButton .helpButtonEnabled:hover::before,
.embeddedServiceHelpButton .helpButton .helpButtonEnabled:focus::before {
    border-radius: 0;
    background-color: transparent;
}

.embeddedServiceHelpButton .helpButton .uiButton.helpButtonEnabled,
.embeddedServiceHelpButton .helpButton .uiButton.helpButtonDisabled {
    padding: 12px 20px !important;
    background-color: var(--color-black-600);
    justify-content: center !important;
    gap: 5px !important;
    box-shadow: none;
    max-width: 210px;
    min-width: 210px;
    width: 210px;
    border-radius: 0 !important;
    height: 2.50rem !important;
}

.embeddedServiceHelpButton .helpButton .uiButton.helpButtonEnabled:hover,
.embeddedServiceHelpButton .helpButton .uiButton.helpButtonEnabled:focus-visible,
.embeddedServiceHelpButton .helpButton .uiButton.helpButtonEnabled:focus{
    background-color: var(--color-black-700);
    border-radius: 0 !important;
    box-shadow: none;
    outline: 0.25rem solid var(--color-primary);
    outline-offset: 0.25rem;
}

.embeddedServiceHelpButton .helpButtonEnabled .embeddedServiceIcon,
.embeddedServiceHelpButton .helpButtonDisabled .embeddedServiceIcon {
    margin-right: 0;
}

.embeddedServiceHelpButton .helpButtonEnabled .helpButtonLabel,
.embeddedServiceHelpButton .helpButtonDisabled .helpButtonLabel {
    font-family: var(--font-body);
    font-weight: 400;
    color: var(--color-white);
}

.embeddedServiceHelpButton .helpButtonEnabled .embeddedServiceIcon,
.embeddedServiceHelpButton .helpButtonDisabled .embeddedServiceIcon{
    color: var(--color-white);
}


/**
 * Clicked chat
*/

.embeddedServiceSidebar .sidebarHeader {
   background-color: var(--color-black-600);
}

.embeddedServiceSidebar .sidebarHeader h2 {
   color: var(--color-white);
   font-family: var(--font-body);
   font-weight: 400 !important;
   font-size: 1em !important;
}

.embeddedServiceSidebar .sidebarHeader svg {
   fill: var(--color-white) !important;
}

.embeddedServiceSidebar .sidebarHeader button[embeddedService-chatHeader_chatHeader] {
   margin-top: -4px;
   height: unset;
}

.embeddedServiceSidebar.layout-docked .dockableContainer {
    border-radius: 0 !important;
}

button.dialogButton.dialog-button-0.uiButton.embeddedServiceSidebarButton,
button.endChatButton.closeChatButton.uiButton--default.uiButton.embeddedServiceSidebarButton {
    background-color: var(--color-black-600);
    border-radius: 0;
}

button.dialogButton.dialog-button-0.uiButton.embeddedServiceSidebarButton:hover,
button.dialogButton.dialog-button-0.uiButton.embeddedServiceSidebarButton:focus-visible,
button.dialogButton.dialog-button-0.uiButton.embeddedServiceSidebarButton:focus,
button.endChatButton.closeChatButton.uiButton--default.uiButton.embeddedServiceSidebarButton:hover,
button.endChatButton.closeChatButton.uiButton--default.uiButton.embeddedServiceSidebarButton:focus-visible,
button.endChatButton.closeChatButton.uiButton--default.uiButton.embeddedServiceSidebarButton:focus{
    background-color: var(--color-black-700);
    outline: 0.25rem solid var(--color-primary);
    outline-offset: 0.25rem;
}


/**
 * Active chat minimized
*/

.embeddedServiceSidebar .sidebarHeader.minimizedContainer.embeddedServiceSidebarMinimizedDefaultUI {
    border-width: 0;
    border-color: var(--color-black-600);  
    border-top-left-radius: unset;
    border-top-right-radius: unset;
    box-shadow: none;
    width: 210px;
}

.embeddedServiceSidebar .sidebarHeader.minimizedContainer.embeddedServiceSidebarMinimizedDefaultUI:hover,
.embeddedServiceSidebar .sidebarHeader.minimizedContainer.embeddedServiceSidebarMinimizedDefaultUI:focus-visible,
.embeddedServiceSidebar .sidebarHeader.minimizedContainer.embeddedServiceSidebarMinimizedDefaultUI:focus {
    background-color: var(--color-black-700);
    width: 210px;
    text-decoration: none;
    outline: 0.25rem solid var(--color-primary);
    outline-offset: 0.25rem;
}

.embeddedServiceSidebar .sidebarHeader.minimizedContainer.embeddedServiceSidebarMinimizedDefaultUI .message {
   color: var(--color-white);
   font-family: var(--font-body);
   font-weight: 400;
   font-size: 1em !important;
}


/**
 * Pre-chat form
*/

button.embeddedServiceSidebarButton.uiButton--default.uiButton {
    background-color: var(--color-black-600);
    border-radius: 0;
}

.uiMessage {
  background-color: var(--color-black-600);
  font-family: var(--font-body);
  font-size: .95em !important;
  padding: 1rem;
  margin: 0.5rem;
}

.uiMessage .uiBlock .bLeft {
  display: none;
}

.embeddedServiceHelpButton .helpButtonEnabled .helpButtonLabel,
.embeddedServiceHelpButton .helpButtonDisabled .helpButtonLabel {
  font-family: var(--font-body);
  font-weight: 400;
  color: var(--color-white);
}

.prechatUI {
    height: 80%;
}


.cPreChatForm input[type="text"], 
.cPreChatForm input[type="email"], 
.cPreChatForm input[type="tel"] {
display: block;
  border: 1px solid var(--color-black-600);
  width: 95%;
  font-family: var(--font-body);
  font-weight: 400 !important;
  font-size: 1rem !important;
  padding: .5rem;
  margin-bottom: .75rem;
}

.cPreChatForm label {
  display: flex;
  font-family: var(--font-body);
  font-weight: 400 !important;
  font-size: .95rem !important;
  left: 0 !important;
  padding: 0;
  margin: 0 !important;
}

.cPreChatForm label.label-left {
  margin-right: 1rem !important; 
}

.cPreChatForm .required {
  font-weight: 700 !important;
  font-size: 1.3em !important;
  position: unset !important;
  padding-left: .3rem !important;
  
}

.cPreChatForm .uiInput input.slds-hide {
  display: none;
}

.embeddedServiceLiveAgentStateChatItem .isLightningOutContext .avatar {
  background-color: var(--color-black-600);
}


.scrollDownButton .slds-button {
  background: unset;
  background-color: var(--color-black-600) !important;
  color: var(--color-white);
}

.scrollDownButton .slds-button .slds-button__icon {
  color: var(--color-white);
}

</style>
