/* Fonts */

@font-face {
    font-family: "GreycliffCFBuildEmpire";
    src: url("/wp-content/themes/salient-child/fonts/greycliffcfbuildempire-extrabold-webfont.woff2") format("woff2"),
         url('/wp-content/themes/salient-child/fonts/greycliffcfbuildempire-extrabold-webfont.woff2') format('woff');
    font-weight: 600;
    font-style: normal;
  }
  
  @font-face {
    font-family: "GreycliffCFBuildEmpire";
    src: url("/wp-content/themes/salient-child/fonts/greycliffcfbuildempire-medium-webfont.woff2") format("woff2"),
         url('/wp-content/themes/salient-child/fonts/greycliffcfbuildempire-medium-webfont.woff') format('woff');
    font-weight: 500;
    font-style: normal;
  }
  
  h1, h2, h3, h4, h5, h6 { 
      font-family: GreycliffCFBuildEmpire;
      font-weight: 600;
  }
  
  .d-none {
      display: none;
  }
  
  body, a, blockquote { 
      font-family: GreycliffCFBuildEmpire;
      font-weight: 500;
  }
  
  strong,
  b,
  button[type=submit],
  input[type=button],
  input[type=submit] {
      font-family: GreycliffCFBuildEmpire !important;
  }
  
  .nectar-button {
      font-family: GreycliffCFBuildEmpire;
      font-weight: 500;
  }
  
  .nectar-button.small {
      font-size: 14px;
      padding: 9px 22px;
  }
  
  /* footer */
  
  @media (min-width: 690px) {
      .span_1_5 {
          width: 19.2%;
          margin-right: 1%;
      }
  }
  
  .widget-title {
      color: #bdbdbd;
  }
  
  #footer-outer .content .policies {
      display: flex;
      gap: 15px;
  }
  
  #footer-outer .content img {
      max-width: 100px;
  }
  
  #footer-outer .content {
      display: flex;
      gap: 40px;
      align-items: center;
  }
  
  .topper .hover-wrap {
      margin-bottom: 0;
  }
  
  .grecaptcha-badge {
      display: none !important;
  }
  
  /* text colours */
  
  .c-orange {
      color: #eb7300;
  }
  
  /* buttons */
  
  body[data-button-style*="slightly_rounded"] #top nav > ul > li[class*="button_solid_color"] > a:before {
      border-radius: 50px !important;
  }
  
  /* tags */
  
  .tag:after {
      text-transform: uppercase;
      font-size: 7px;
      font-weight: 600;
      border-radius: 50px;
      padding: 2px 6px;
      letter-spacing: 0.5px;
      align-self: flex-start; 
      line-height: initial;
      border: 1.5px solid;
  }
  
  .tag.new:after {
      content: "new";
      color: #d62680;
      border-color: #d62680;
  }
  
  .tag.hiring:after {
      content: "hiring";
      color: #eb7300;
      border-color: #eb7300;
      display: none;
  }
  
  .tag.get-started:after {
      content: "get started";
      color: #7ac343;
      border-color: #7ac343;
  }
  
  .tag.coming-soon:after {
      content: "coming soon";
      color: #947dc0;
      border-color: #947dc0;
  }
  
  .tag {
      display: flex;
      gap: 5px;
  }
  
  /* mega menu */
  
  #header-outer nav>ul>.megamenu.nectar-megamenu-menu-item.width-50>ul {
    width: 75em;
  }
  
  #header-outer nav > ul > .megamenu > ul li.solutions ul li a:hover {
    background-color: #f9efe0 !important;
  }
  
  #header-outer li.solutions .nectar-menu-label {
    margin: 0 0 5px 0;
  }
  
  #header-outer li.solutions span.nectar-menu-icon-text {
    display: flex;
    flex-direction: column;
  }
  
  #header-outer li.platform .sub-menu {
      right: 15% !important;
  }
  
  .nectar-menu-label {
      padding: 0;
      line-height: 21px;
      font-size: 13px;
      margin-left: 0;
      position: relative;
      display: inline-block;
      vertical-align: middle;
      margin: 10px 0 20px 0;
  }
  
  #header-outer li > a .nectar-menu-label:before {
      background: transparent !important;
  }
  
  #header-outer .nectar-megamenu-menu-item .sub-menu {
      border-radius: 10px;
      border: 0 !important;
  }
  
  #header-outer .nectar-megamenu-menu-item > .sub-menu > #menu-item-165:after,
  #header-outer .nectar-megamenu-menu-item > .sub-menu > #menu-item-172:after,
  #header-outer .nectar-megamenu-menu-item > .sub-menu > #menu-item-4156:after {
      content: "";
      height: 100%;
      width: 1px;
      background: #dfdfdf;
      display: block;
      position: absolute;
      right: 0;
      top: 0;
  }
  
    #header-outer li .sub-menu li > a.sf-with-ul > span.menu-title-text {
        font-size: 18px;
        font-weight: 600;
    }
  
  #header-outer li.menu-item-188 > a .menu-title-text {
      font-size: 30px;
      line-height: 38px;
      font-weight: 600;
  }
  
  #header-outer li.menu-item-198 > a .nectar-menu-label {
      color: #ffffff;
      margin: 0;
  }
  
  #header-outer nav>ul>.megamenu ul #menu-item-172 .sub-menu li a,
  #header-outer nav>ul>.megamenu ul #menu-item-165 .sub-menu li a {
      display: flex;
      align-items: center;
  }
  
  @media (min-width: 690px) {
      #header-outer .col-6 {
          width: 50% !important;
          float: left !important;
      }
  }
  
  #header-outer .col-6 .sf-with-ul {
      padding: 0 !important;
  }
  
  #header-outer li .sub-menu .sf-with-ul > .menu-title-text {
      display: flex;
  }
  
  #header-secondary-outer .nectar-center-text {
      font-size: 13px !important;
      line-height: 13px !important;
      display: flex;
      justify-content: center;
      align-items: center;
      gap: 5px;
  }
  
  #header-secondary-outer .nectar-center-text .tag {
      color: rgb(235 116 0);
      padding: 4px 8px;
      background: rgb(255 209 164);
      border-radius: 100px;
      font-size: 12px;
  }
  
  #header-secondary-outer .nectar-center-text a {
      text-decoration: underline;
      text-underline-offset: 3px;
  }
  
  #header-secondary-outer, #header-outer #header-secondary-outer,
  body #header-outer #mobile-menu .secondary-header-text {
      background: linear-gradient(125deg,#eb7300,#fab400);
  }
  
  #footer-outer .menu-item {
      font-size: 16px;
  }
  
  .material #footer-outer #footer-widgets .col ul li {
      padding: 8px 0px !important;
  }
  
  .menu-item-172 .sub-menu {
      column-count: 2;
  }
  
  @media (max-width: 1000px) {
     .nectar-menu-label {
          display: none !important;
      }
  }
  
  /* cta global section */
  
  .cta-card-img {
      max-width: 200% !important;
  }
  
  /* blog slider - home page */
  
  .blog-slider .meta-category {
      display: none !important;
  }
  
  .blog-slider .masonry.material .masonry-blog-item .article-content-wrap {
      padding: 75px 30px 30px 30px !important;
      position: relative;
  }
  
  .blog-slider .excerpt {
      display: none;
  }
  
  .blog-slider h3.title {
