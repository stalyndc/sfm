#vf-tray > div > div > div > div > article > nav > div.vf-horiz-list.vf-tabbed-nav__tabs.vf-flex-grow > div:nth-child(2) {
    display: none;
}
button[data-test="vf-tray-tab-settings"] {
    display: none !important;
}

/* this hides the email sign in the user profile in the tray*/
.vf-client-settings-email-notifications {
  display: none !important;
}

/* Hide the Settings button in Viafoura comments module */
.vf-tray-settings-button,
span.vf-tray-settings-button,
