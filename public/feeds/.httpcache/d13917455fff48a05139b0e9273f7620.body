                sizes="(max-width: 50em) 98vw, 700px">
              <source type="image/webp"
                srcset="https://images.ctfassets.net/f20lfrunubsq/5ob0VH08H1hSEQyKBrIPng/c29ad0ef1171750c1379af741936327b/Screenshot_2025-07-15_at_22.34.17.png?fm=webp&fit=scale&q=75&w=300&h=130 300w, https://images.ctfassets.net/f20lfrunubsq/5ob0VH08H1hSEQyKBrIPng/c29ad0ef1171750c1379af741936327b/Screenshot_2025-07-15_at_22.34.17.png?fm=webp&fit=scale&q=75&w=500&h=218 500w, https://images.ctfassets.net/f20lfrunubsq/5ob0VH08H1hSEQyKBrIPng/c29ad0ef1171750c1379af741936327b/Screenshot_2025-07-15_at_22.34.17.png?fm=webp&fit=scale&q=75&w=700&h=305 700w, https://images.ctfassets.net/f20lfrunubsq/5ob0VH08H1hSEQyKBrIPng/c29ad0ef1171750c1379af741936327b/Screenshot_2025-07-15_at_22.34.17.png?fm=webp&fit=scale&q=75&w=900&h=392 900w, https://images.ctfassets.net/f20lfrunubsq/5ob0VH08H1hSEQyKBrIPng/c29ad0ef1171750c1379af741936327b/Screenshot_2025-07-15_at_22.34.17.png?fm=webp&fit=scale&q=75&w=1100&h=480 1100w"
