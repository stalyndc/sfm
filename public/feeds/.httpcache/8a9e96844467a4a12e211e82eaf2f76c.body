      url="https://images.ladbible.com/resize?type=jpeg&amp;quality=70&amp;width=808&amp;fit=contain&amp;gravity=auto&amp;url=https://images.ladbiblegroup.com/v3/assets/blt949ea8e16e463049/bltd1b686aeacd6c26d/68b804d21e049aba9a1459df/dads-reaction.png"
      width="720"
      height="404" />
  <media:content url="https://images.ladbible.com/resize?type=jpeg&amp;quality=70&amp;width=1204&amp;fit=contain&amp;gravity=auto&amp;url=https://images.ladbiblegroup.com/v3/assets/blt949ea8e16e463049/bltd1b686aeacd6c26d/68b804d21e049aba9a1459df/dads-reaction.png">
      <media:description type="plain"><![CDATA[Dad's emotional reaction to finding out wife had anonymously bullied and harassed their daughter online for a year]]></media:description>
  </media:content>
