      function setFavorites(articleType, articleId) {
        if (!articleType || !articleId) {
          return;
        }
        const favoritesButtons = document.querySelectorAll(`button[data-article-type="${articleType}"][data-article-id="${articleId}"]`);
        if (favoritesButtons.length === 0) {
          return;
        }

        const favoriteArticlesTtl = parseInt(
          localStorage.getItem('favorite-articles-ttl') || (Date.now() - 1000).toString(),
          10,
        );
        if (favoriteArticlesTtl < Date.now()) {
          return;
        }

        const favoritesArticles = JSON.parse(localStorage.getItem('favorite-articles') || '[]'),
          articleKey = favoritesButtons[0].dataset.articleType.concat('-', favoritesButtons[0].dataset.articleId),
          isFavorite = favoritesArticles.includes(articleKey);

        favoritesButtons.forEach((btn) => {
          btn.dataset.isActive = isFavorite.toString();
          btn.dataset.tagname = `${isFavorite ? 'suppression' : 'ajout'}_favori`;
        });
      }
