<script type="899ce32e6ce80b38d7a651a5-text/javascript">
    jQuery(document).ready(function($) {
        // Add a small delay to ensure proper initialization
        setTimeout(function() {
            const mainCarousel = $('.main-carousel').owlCarousel({
                items: 1,
                nav: false,
                dots: false,
                loop: true,
                stagePadding: 0,
                autoWidth: false, // Ensure carousel respects items setting
                center: false, // Prevent centering issues
            });

            // Force refresh after initialization
            setTimeout(function() {
                mainCarousel.trigger('refresh.owl.carousel');
            }, 100);

            const thumbnailCarousel = $('.thumbnail-carousel').owlCarousel({
                items: 4,
                nav: false,
                dots: false,
                loop: true,
                margin: 4,
                responsive: {
                    0: {
                        items: 2
                    },
                    768: {
                        items: 3
                    },
                    992: {
                        items: 4
                    }
                }
            });

            // Sync thumbnails with main carousel
            $('.thumbnail-item').on('click', function() {
                // Get the real index considering clone elements in owl carousel
                let index = $(this).parent().index();
                const itemCount = $('.post-title').length;

                // Handle loop situation (owl carousel clones items)
                index = index % itemCount;

                mainCarousel.trigger('to.owl.carousel', [index, 300]);
                thumbnailCarousel.trigger('to.owl.carousel', [index, 300]);

                // Update active title
                $('.post-title').removeClass('active');
                $('.post-title[data-slide="' + index + '"]').addClass('active');
            });

            // Update active thumbnail when main carousel changes
            mainCarousel.on('changed.owl.carousel', function(event) {
                // Get the actual index (considering clones in loop mode)
                const itemCount = event.item.count;
                const index = event.item.index % itemCount;

                // Update thumbnails
                $('.thumbnail-item').removeClass('active');
                $('.thumbnail-item').eq(index).addClass('active');
                thumbnailCarousel.trigger('to.owl.carousel', [index, 300, true]);

                // Update title
                $('.post-title').removeClass('active');
                $('.post-title[data-slide="' + index + '"]').addClass('active');
            });

            // Set initial active thumbnail
            $('.thumbnail-item').first().addClass('active');

            // Add click handler for post titles
            $('.post-title').on('mouseenter', function() {
                const index = $(this).data('slide');
                mainCarousel.trigger('to.owl.carousel', [index, 300]);
                thumbnailCarousel.trigger('to.owl.carousel', [index, 300]);

                // Update active state of titles
                $('.post-title').removeClass('active');
                $(this).addClass('active');
            });

            // Update active title when main carousel changes
            mainCarousel.on('changed.owl.carousel', function(event) {
                const index = event.item.index % event.item.count;
                $('.post-title').removeClass('active');
                $('.post-title').eq(index).addClass('active');
            });

            // Add this after the mainCarousel initialization

        }, 50); // Close the setTimeout with 50ms delay
    });
</script>				</div>
