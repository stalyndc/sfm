    <script>
        $(document).ready(function() {
            $('#addNewsletter').submit(function(e) {
                e.preventDefault();
                var form = $(this);

                var submitButton = form.find('button[type="submit"]');
                submitButton.prop('disabled', true);

                $.ajax({
                    url: form.attr('action'),
                    type: form.attr('method'),
                    data: form.serialize(),
                    success: function(response) {
                        submitButton.prop('disabled', false);
                        Swal.fire({
                            icon: 'success',
                            text: response.message,
                        }).then(function(result) {
                            form.trigger('reset');
                            window.location.reload();
                        });
                    },
                    error: function(response) {
                        submitButton.prop('disabled', false);
                        var errorMessages = '';

                        // Gestion des erreurs de validation
                        if (response.status === 422) {
                            var errors = response.responseJSON.errors;
                            $.each(errors, function(key, messages) {
                                errorMessages += messages.join('<br>') + '<br>';
                            });
                        }

                        // Gestion des erreurs personnalisées
                        if (response.status === 400 || response.status === 500) {
                            var customError = response.responseJSON.error;
                            errorMessages += customError + '<br>';
                        }

                        Swal.fire({
                            icon: 'error',
                            title: 'Erreur!',
                            html: errorMessages, // Utilisez html pour afficher des lignes multiples
                        });

                    }
                });
            });
        });

        function removeSpaces(input) {
            input.value = input.value.replace(/\s/g, ''); // Supprime tous les espaces
        }
    </script>

