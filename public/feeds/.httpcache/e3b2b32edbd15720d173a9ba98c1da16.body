function loadFancybox(callback) {
    if (isFancyboxLoaded()) {
        callback(); // If FancyBox is already loaded, just run the callback
        return;
    }

    // Dynamically load FancyBox CSS
    const fancyboxCSS = document.createElement('link');
    fancyboxCSS.rel = 'stylesheet';
