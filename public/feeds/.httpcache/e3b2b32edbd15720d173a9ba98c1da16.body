    for (let i = 0; i < jumpMenus.length; i++) {
        const jumpMenu = jumpMenus[i];
        if (!jumpMenu || jumpMenu.style.display === 'none') continue;

        const existingSpacer = jumpMenu.parentNode.querySelector('.jump-menu-spacer');
        if (existingSpacer) {
