    <script>
        const replaceQueryParam = (param, newval, search) => {
            let regex = new RegExp("([?;&])" + param + "[^&;]*[;&]?");
            let query = search.replace(regex, "$1").replace(/&$/, '');
            return (query.length > 2 ? query + "&" : "?") + (newval ? param + "=" + newval : '');
        }
        const fe_load_posts = (limit, offset, callbackFunction) => {
            let axiosUri = '/fe-provider?';
            axiosUri = replaceQueryParam('limit', limit, axiosUri);
            axiosUri = replaceQueryParam('offset', offset, axiosUri);
            axios.get(axiosUri).then(function (response) {
                if(response.status === 200){
                    callbackFunction(response);
                }
            })
        }
    </script>
</section>    <style>
    #fe-load-js {
        display: flex;
        align-content: center;
        text-align: center;
        width: 180px;
        height: 25px;
    }
    #fe-load-js span,
    #fe-load-js .lds-ring
    {
        margin: 0 auto;
    }
    .lds-ring {
        display: inline-block;
        position: relative;
        width: 30px;
        height: 30px;
    }
    .lds-ring div {
        box-sizing: border-box;
        display: block;
        position: absolute;
        width: 20px;
        height: 20px;
        margin: 3px;
        border: 3px solid #fff;
        border-radius: 50%;
        animation: lds-ring 1.2s cubic-bezier(0.5, 0, 0.5, 1) infinite;
        border-color: #fff transparent transparent transparent;
    }
    .lds-ring div:nth-child(1) {
        animation-delay: -0.45s;
    }
    .lds-ring div:nth-child(2) {
        animation-delay: -0.3s;
    }
    .lds-ring div:nth-child(3) {
        animation-delay: -0.15s;
    }
    @keyframes lds-ring {
        0% {
            transform: rotate(0deg);
        }
        100% {
            transform: rotate(360deg);
        }
    }

</style>
<section id="load-section">
    <div class="fe-container load-section-wrapper">
        <div class="fe-load-img">

        </div>
        <a href="#" id="fe-load-js" class="fe-btn btn-black"><span>READ MORE POSTS</span></a>
    </div>
    <script>
        let limit = 4;
        let offset = 4;
        let ctaButtonLoad = document.querySelector('#fe-load-js');
        ctaButtonLoad.addEventListener('click', (e) => {
            e.preventDefault();
            ctaButtonLoad.innerHTML = '<div class="lds-ring"><div></div><div></div><div></div><div></div></div>';
            fe_load_posts(limit, offset, response => {
                let postsWrap = document.querySelector('.fe-blog-wrapper');
                postsWrap.innerHTML += response.data;
                offset += limit;
                ctaButtonLoad.innerHTML = '<span>READ MORE POSTS<span>';
                let helperTimeout = setTimeout(() => {
                    postsWrap.querySelectorAll('.fe-animate').forEach(postAnim => {
                        if(postAnim.classList.contains('fe-animate')){
                            postAnim.classList.remove('fe-animate');
                        }
                    })
                    clearTimeout(helperTimeout);
                }, 2000);
            });
        })
    </script>
