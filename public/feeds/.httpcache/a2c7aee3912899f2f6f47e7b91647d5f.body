            const headerSignupPopup = () => {
                const ctaClass = 'popup-opened';
                const animate = 'animate-popup';
                let signPopup = document.querySelector('.fe-sign-popup');
                let headerSignup = document.querySelector('.menu-signup');
                let closePopup = document.querySelector('.close-popup-fe');
                headerSignup.addEventListener('click', (e) =>{
