                let liMenuSearch = document.querySelector('.menu-search');
                liMenuSearch.innerHTML = searchFormHtml;
                let searchFormSent = document.querySelector('#searchform');
                let isOpened = true;
                if(window.innerWidth > 768){
                    isOpened = false;
                }
                let formInput = searchFormSent.querySelector('#s');
                searchFormSent.addEventListener('submit', (e) => {
                    e.preventDefault();
                    if(!isOpened){
                        isOpened = !isOpened;
                        if(window.innerWidth > 768){
                            formInput.style.width = '120px';
                        }
                        formInput.focus();
                    }else{
                        searchFormSent.submit();
                    }
                })
                formInput.addEventListener('focusout', (e) => {
                    e.preventDefault();
                    formInput.style.width = '';
                    let helperTime = setTimeout(() => {
                        isOpened = !isOpened;
                        clearTimeout(helperTime);
                    }, 1000);
                });
            }
            const burgerJs = () => {
                let burgerContainer = document.querySelector('#burger-container');
                let navContainer = document.querySelector('.navigation .menu-header-container');
